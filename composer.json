{
    "name": "mobilerider/aventri-php-sdk",
    "type": "library",
    "description": "PHP client library to consume Aventri services",
    "version": "0.1.0-alpha.1",
    "keywords": ["aventri", "api", "registration", "atendee"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yojahny Chavez",
            "email": "yojahnychavez@gmail.com"
        },
        {
            "name": "Michel Perez",
            "email": "michel@mobilerider.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mr\\AventriSdk\\": "src/",
            "Mr\\AventriSdkTest\\": "tests/"
        }
    },
    "repositories": [
